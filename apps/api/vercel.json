{
  "version": 2,
  "builds": [
    {
      "src": "../../api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "../../api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "../../api/index.js"
    }
  ],
  "installCommand": "echo 'No dependencies needed for serverless API'",
  "buildCommand": "echo 'Serverless API ready for deployment'",
  "github": {
    "autoAlias": false
  }
}